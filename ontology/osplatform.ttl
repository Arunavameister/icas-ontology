<http://www.invincea.com/ontologies/icas/1.0/osplatform> 
			rdf:type owl:Ontology ;                                           
			rdfs:label "Operating Systems and Platforms Ontology"@en ;
			rdfs:comment "Elements relating to the combination of hardware and software that describe the software environment provided by a host." ;                                           
			dc:description "Elements relating to the combination of hardware and software that describe the software environment provided by a host."@en ;
			owl:versionInfo "1.0" ;
			dc:creator: "Invincea Labs"@en ;
			dc:date "2014" .

			
:installedOS 
			rdf:type owl:ObjectProperty , owl:FunctionalProperty;             
			rdfs:domain host:Host ;
			rdfs:range :OS ;
			rdfs:label "Installed Operating System"@en ;
			tapio:signaturePriority 1 ; 
			rdfs:comment ""@en .

:OS 
			rdf:type owl:Class ;
			rdfs:label "Operating System"@en ;
			rdfs:comment "A specific release of an Operating System"@en .

:hasOSFamily 
			rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
			rdfs:domain :OS ;
			rdfs:range :OSFamily ;
			tapio:signaturePriority 2 ;
			rdfs:label "The family this operating system belongs to"@en.

:OSFamily 
			rdf:type owl:Class ;
			rdfs:subClassOf owl:Thing ;
			rdfs:label "Operating System Family"@en ;
			rdfs:comment "A Family of Operating System versions"@en .

:Platform 
			rdf:type owl:Class ;
			rdfs:subClassOf owl:Thing ;
			rdfs:label "Platform"@en ;
			rdfs:comment "A Platform is a combination of operating system family, release and underlying hardware, that determines the environment in which software executes"@en .

