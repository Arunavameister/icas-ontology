:IMAP
			rdf:type :Layer7 , owl:NamedIndividual ;
			rdfs:label "IMAP"@en ;
			rdfs:comment "Internet Mail Access Protocol"@en .

:IPP
			rdf:type :Layer7 , owl:NamedIndividual ;
			rdfs:label "IPP"@en ;
			rdfs:comment "Internet Printing Protocol"@en .

:POP3
			rdf:type :Layer7 , owl:NamedIndividual ;
			rdfs:label "POP3"@en ;
