:TCP 
			rdf:type :Layer4 ,  owl:NamedIndividual ;
			rdfs:label "TCP"@en ;
			rdfs:label "Transmission Control Protocol"@en ;
			:hasIPProtocolNumber  6 .
:Telnet 
			rdf:type :Layer7 , owl:NamedIndividual ;
			rdfs:label "Telnet"@en ;
			rdfs:comment "Telnet Protocol"@en .

:DNS
			rdf:type :Layer7 , owl:NamedIndividual ;
			rdfs:label "DNS"@en ;
